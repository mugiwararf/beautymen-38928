<main class="main">
  <div class="inner">
         <% if user_signed_in? %>
      <div class="greeting">
        Menz Beauty（メンズビューティー）にようこそ!
        <%= link_to "#{current_user.nickname}さん", user_path(current_user.id), class: :greeting__link %>
      </div>
      <% end %>
    <div class="wrapper">
      <div class="container">
      <div class="container__item">
      <%= image_tag("recommend_item1.png", class: :card__imgs )  %>
      </div>
      <div class="container__item">
      <%= image_tag("KkJv0hc.png", class: :card__imgs )  %>
      </div>
      <div class="container__item">
      <%= image_tag("picture_pc.png", class: :card__imgs )  %>
       </div>
      <div class="container__item">
       <%= image_tag("87412-11.png", class: :card__imgs )  %>
      </div>
      </div>
    </div>
    <div class="card__wrapper">
       <%= render partial: "prototype", collection: @prototypes %>
    </div>
  </div>
</main>
